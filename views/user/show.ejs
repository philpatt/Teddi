<header class="text-center showHeader">More information about <%=results.dbInfo.fullName%>...</header>
		<div class="showCont row">
			<div class='col-xs-6 showInfo'>
				<h2>Weather Info</h2>
				<p><%=results.dbInfo.weatherInfo%></p>
			</div>
			<div class='col-xs-6 showInfo'>
				<h2>Directions</h2>
				<p><%=results.dbInfo.directionsInfo%></p>
			</div>
			<div class='col-xs-6 showInfo'>
				<h2 style='color:red'>teddi Alerts</h2>
				<% results.data.forEach(function(item){ %>
					<%if(item.title === ''){ %>
						<h5>No alerts contact NPS for information</h5>
					<% } else { %>
						<h5 style="color:red">Importance: <%=item.category %></h5>
						<h5> <%=item.title %></h5>
						<p> <%=item.description %></p>
						<p>for more info <a href="<%= item.url %>">click here</a> </p>
						<hr>
					<%}%>
				<% }); %>
			</div>
			<a href ='/favorites/<%= results.dbInfo.id %>' class='show-del btn btn-danger delete-link '>remove from favorites</a>
		</div>








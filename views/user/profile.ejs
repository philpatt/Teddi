<div class='col-xs-4'>
	<h2>Hey there, <%= currentUser.firstname %> !</h2>
	<hr>
	<% user.parks.forEach(function(item){ %>
		<div class=" text-center">
			<ul class='info well'>
				<h3><%= item.fullName %></h3>
				<p><%= item.description %></p>
				<p><%= item.parkCode %></p>
			</ul>
			<form id='search-form' action="/show/show">
				<input hidden name='parkCode' type="text" value="<%= item.parkCode %>">
				<button type="text">More dDetails</button>
			</form>
		
		</div>
	<hr>
		<% }); %>
</div>

